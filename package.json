{
  "publisher": "lokray",
  "name": "nebula-lang",
  "displayName": "Nebula",
  "description": "Provides syntax highlighting for Nebula (.neb) files",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nebula",
        "aliases": [
          "Nebula",
          "nebula"
        ],
        "extensions": [
          ".neb"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nebula",
        "scopeName": "source.nebula",
        "path": "./syntaxes/nebula.tmLanguage.json"
      }
    ]
  }
}